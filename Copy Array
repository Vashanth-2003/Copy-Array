import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char[] copyFrom = {'g','r','e','a','t','t','e','r','v','p','r','o','i','s','h','e','r','e'};
	    char[] copyTo = new char[7];
	    System.arraycopy(copyFrom, 5, copyTo, 0, 7);
	    System.out.println(String.valueOf(copyTo));
	}
}
